<h1>
    Welcome to Your NoteTaking Space
</h1>
<form action="/postNotes" method="POST">
    <div><label for="title">Title:</label><input type="text"  name="title" id="title" ></div>
	<div><label for="content">Content:</label><textarea name="content" id="content" cols="30" rows="10"></textarea></div>
    <input type="submit" value="Submit">
 </form> 


    {{#each note}}
        <div class="notes">
            <div class="note-header"><span class="note-title">{{title}}</span><span class="note-cross"><i class="fas fa-times"></i></span></div>
            <div class="note-body">{{content}}</div>
        </div>
    {{/each}}
</div>
